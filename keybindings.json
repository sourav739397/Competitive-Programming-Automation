// Place your key bindings in this file to override the defaults
[
    {
        "key": "alt+r", // test solution
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "cprun --cp \"${fileBasename}\"\n"
        }
    },
    {
        "key": "alt+d", // test solutio with debug 
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "cprun --cp \"${fileBasename}\" -d\n"
        }
    },
    {
        "key": "alt+f", // fetch testcase
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "cprun --parse here\n"
        }
    },
    {
        "key": "alt+c", // run file
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "cprun \"${fileBasename}\" -d\n"
        }
    },
    {
        "key": "alt+e", // run file without compile(c, c++)
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "cprun \"${fileBasename}\" --cp -r\n"
        }
    }
]